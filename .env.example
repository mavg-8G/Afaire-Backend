# Example .env for development and production

# Common
SECRET_KEY="your-secret-key"
APP_ENV=development
GITHUB_SECRET="your-github-secret"
GITHUB_SECRET_FRONT=""

# For production (PostgreSQL)
POSTGRES_USER=youruser
POSTGRES_PASSWORD=yourpassword
POSTGRES_DB=yourdb
POSTGRES_HOST=localhost  # use when running container with --network host (Linux)
# POSTGRES_HOST=host.docker.internal  # use with default bridge and --add-host host-gateway
POSTGRES_PORT=5432
